<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="point_of_sale.ControlButtons" t-inherit="point_of_sale.ControlButtons" t-inherit-mode="extension">
        <xpath expr="//button[contains(@class, 'control-button')]" position="before">
            <button class="btn btn-light control-button" t-on-click="onClickNcf">
                <i class="fa fa-file-text-o"/>
                NCF
            </button>
        </xpath>
    </t>
</templates>
